{{template "header"}}

<h1>Welcome to SongVote</h1>

<p>Below are the songs in the database:</p>

<table class="table table-hover table-sm">
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Votes</th>
    <th>Veto</th>
  </tr>
  {{range .}}
  <tr>
    <td><a href="{{.LinkURL}}">{{.Name}}</a></td>
    <td>{{.Artist}}</td>
    <td>{{.Votes}}</td>
    <td>{{.Vetoed}}</td>
  </tr>
  {{end}}
</table>

{{template "footer"}}
